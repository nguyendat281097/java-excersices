package main;

import java.util.Arrays;

public class Main {

	public int solution(int[] a) {
		Arrays.sort(a);
		int ai, aj, result = 1;
		for (int i = 0; i < a.length - 1; i++) {
			ai = a[i];
			aj = a[i + 1];
			if (ai <= 0) {
				continue;
			}
			if (ai == aj || aj == (ai + 1)) {
				continue;
			} else {
				result = ai + 1;
			}
		}
		if (result <= a[a.length - 1])
			return result;
		return -1;
	}

}
